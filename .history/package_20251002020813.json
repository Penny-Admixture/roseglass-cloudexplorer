{
  "name": "roseglass-cloudexplorer",
  "version": "1.0.0",
  "description": "A robust Google Drive client built on Rclone with AirCluster-like functionality",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "setup": "npm install && cd client && npm install && cd .. && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "ws": "^8.14.2",
    "execa": "^8.0.1",
    "chokidar": "^3.5.3",
    "sqlite3": "^5.1.6",
    "node-cron": "^3.0.3",
    "archiver": "^6.0.1",
    "extract-zip": "^2.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "@types/uuid": "^9.0.7"
  },
  "keywords": ["google-drive", "rclone", "file-manager", "cloud-storage"],
  "author": "RoseGlass",
  "license": "MIT"
}
